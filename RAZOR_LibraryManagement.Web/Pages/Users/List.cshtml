@page
@model RAZOR_LibraryManagement.Web.Pages.Users.ListModel
@{
}


<div class="container">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>
                    User name
                </th>
                <th>
                    Email
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model.vmUserIndexList)
            {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                </tr>
            }
        </tbody>
    </table>
</div>